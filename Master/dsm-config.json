{
  "name": "Docker Service Manager",
  "version": "0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/ThejanW/Docker-Service-Manager.git"
  },
  "license": "MIT",
  "dsm_port": "8765",
  "base_url": "unix://var/run/docker.sock",
  "reverse_proxy": {
    "name": "jwilder/nginx-proxy",
    "version": "0.4.0",
    "volumes": {
      "/var/run/docker.sock": {
        "bind": "/tmp/docker.sock",
        "mode": "ro"
      }
    },
    "ports": {
      "80/tcp": "8764"
    }
  },
  "services": [
    {
      "name": "object_recognition-image",
      "image_name": "thejanw/inception-rest-tika",
      "description": "This is a classifier service that can classify images",
      "virtual_host": "object_recognition-image.tika.dl"
    },
    {
      "name": "object_recognition-video",
      "image_name": "theja/inception-video-rest-tika",
      "description": "This is a classifier service that can classify videos",
      "virtual_host": "object_recognition-video.tika.dl"
    },
    {
      "name": "captioning-image",
      "image_name": "thejanw/im2txt-rest-tika",
      "description": "This is a service that can caption images",
      "virtual_host": "captioning-image.tika.dl"
    }
  ]
}
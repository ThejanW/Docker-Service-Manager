{
  "name": "Docker Service Manager",
  "version": "0.1",
  "repository": {
    "type": "git",
    "url": "git://github.com/ThejanW/Docker-Service-Manager.git"
  },
  "license": "MIT",
  "dsm_port": "8765",
  "base_url": "unix://var/run/docker.sock",
  "reverse_proxy": {
    "name": "jwilder/nginx-proxy",
    "version": "0.4.0",
    "volumes": {
      "/var/run/docker.sock": {
        "bind": "/tmp/docker.sock",
        "mode": "ro"
      }
    },
    "ports": {
      "80/tcp": "8764"
    }
  },
  "services": [
    {
      "name": "demo_service1",
      "image_name": "thejanw/demo-service1",
      "description": "This is demo service 1",
      "virtual_host": "service1.services.dev"
    },
    {
      "name": "demo_service2",
      "image_name": "thejanw/demo-service2",
      "description": "This is demo service 2",
      "virtual_host": "service2.services.dev"
    },
    {
      "name": "demo_service3",
      "image_name": "thejanw/demo-service3",
      "description": "This is demo service 3",
      "virtual_host": "service3.services.dev"
    },
    {
      "name": "demo_ubuntu",
      "image_name": "ubuntu:16.04",
      "description": "This is demo ubuntu",
      "virtual_host": "ubuntu.services.dev"
    }
  ]
}